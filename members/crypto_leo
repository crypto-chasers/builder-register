用任意编程语言计算以下公式 
def cubeRoot(n):
    last = n
    new = last - ((last ** 3 - n) / (3 * last ** 2))
    while abs(new - last) > 0.000001:  # 精度要求
        last = new
        new = last - ((last ** 3 - n) / (3 * last ** 2))
    return new
def cub(n):
    return (n * n * n)

def main():
    sum = 0
    for i in range(100):
        if i == 0:
            continue
        sum = sum + cub(i) - cubeRoot(i)
    print(sum)

main()
